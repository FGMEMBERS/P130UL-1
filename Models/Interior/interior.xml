  <!-- Panel -->
  <model>
    <path>Panel/panel.xml</path>
  </model>

<!--Yoke Animation-->
  <animation>
    <type>rotate</type>
    <object-name>yoke</object-name>
    <property>controls/flight/elevator</property>
    <factor> 15 </factor>
    <center>
      <x-m>  0.25404 </x-m>
      <z-m>-0.78307</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> -1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>yoke</object-name>
    <property>controls/flight/aileron</property>
    <factor> 20 </factor>
    <center>
      <x-m>  0.25404</x-m>
      <z-m>-0.78307</z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
