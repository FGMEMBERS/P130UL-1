<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

<!--Pedal animation-->

  <animation>
    <type>rotate</type>
    <object-name>Palonnier</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  -30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m>  -0.438 </x-m>
      <z-m>-0.78708</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Palonnier.002</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> -30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m> -0.40115</x-m>
      <z-m> -0.79001</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
